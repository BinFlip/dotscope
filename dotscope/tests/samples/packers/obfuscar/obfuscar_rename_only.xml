<?xml version='1.0'?>
<!-- Renaming only: no string hiding, no SuppressIldasm.
     Isolates the renaming engine for testing cleanup/rename recovery. -->
<Obfuscator>
  <Var name="InPath" value="." />
  <Var name="OutPath" value="./output_rename" />

  <!-- Enable renaming (defaults) -->
  <Var name="RenameFields" value="true" />
  <Var name="RenameProperties" value="true" />
  <Var name="RenameEvents" value="true" />

  <!-- Disable everything else -->
  <Var name="HideStrings" value="false" />
  <Var name="SuppressIldasm" value="false" />

  <Module file="$(InPath)/TestApp.exe" />
</Obfuscator>
